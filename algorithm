n = int(input())
a = [0] + list(map(int, input().split()))
d = [[0 for j in range(21)]for i in range(n)]

d[1][a[1]] = 1

for i in range(2, n):
    for j in range(0, 21):

        if j-a[i] >= 0:
            d[i][j] += d[i-1][j-a[i]]
        if j+a[i] <= 20:
            d[i][j] += d[i-1][j+a[i]]

print(d[n-1][a[n]])
