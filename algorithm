#import sys
#input = sys.stdin.readline
#print = sys.stdout.write

t = int(input())

for test_case in range(1, t+1):
    n = int(input())
    d = [0] * (n+1)

    a = list(map(int, input().split()))

    d[0] = a[0]
    result = d[0]
    for i in range(2, n):
        d[i] = max(a[i], d[i-1] + a[i])
        result = max(result, d[i])

    print("#%d %d" % (test_case, result))