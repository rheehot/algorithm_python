import sys
input = sys.stdin.readline
print = sys.stdout.write
sys.setrecursionlimit(10**6)

def go(i):
    if i<=3: return d[i]

    if d[i] > 0: return d[i]

    d[i] = go(i-1) + go(i-2) + go(i-3) + go(i-4)

    return d[i]


d = [0] * 70
d[0] = 1
d[1] = 1
d[2] = 2
d[3] = 4

t = int(input())

for i in range(t):
    n = int(input())
    print("%d\n" % go(n))