#import sys
#input = sys.stdin.readline
#print = sys.stdout.write

t = int(input())

for test_case in range(1, t+1):
    n = int(input())
    a = []
    sum_height = 0
    mid = 0
    result = 0

    for i in range(n):
        num = int(input())
        a.append(num)
        sum_height += num

    mid = sum_height / n

    for i in range(n):
        if a[i] > mid:
            result += (a[i] - mid)

    print("#%d %d" % (test_case, result))