# import sys
# input = sys.stdin.readline
# print = sys.stdout.write

t = int(input())
for test_case in range(1, t+1):
    a = list(map(int, input().split()))
    n = len(a)
    d = [1] * (n+1)

    for i in range(n):
        for j in range(0, i):
            if a[i] > a[j]:
                d[i] = max(d[i], d[j] + 1)

    result = 0
    for i in d:
        result = max(result, i)
    print("#%d %d" % (test_case, result))

