import sys
input = sys.stdin.readline
print = sys.stdout.write
sys.setrecursionlimit(10 ** 6)


def go(i):
    if i == 0: return a[0]

    if d[i] > 0: return d[i]

    d[i] = max(go(i - 1) + a[i], a[i])
    global result
    result = max(result, d[i])

    return d[i]


n = int(input())
a = list(map(int, input().split()))
d = [0] * (n+1)

d[0] = a[0]
result = d[0]


go(n-1)
print("%d\n" % result)
