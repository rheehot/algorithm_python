import sys
input = sys.stdin.readline
print = sys.stdout.write
sys.setrecursionlimit(10**6)


def go(index):
    global sub, pick, cnt
    if index == n:
        if sub == s and pick > 0:
            cnt += 1
        return

    pick += 1
    sub += a[index]
    go(index+1)
    sub -= a[index]
    pick -= 1

    go(index+1)


cnt = 0
sub = 0
pick = 0
n, s = map(int, input().split())
a = list(map(int, input().split()))

go(0)
print("%d\n" % cnt)